{
  "name": "coc-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts-ts": "^4.0.8"
  },
  "scripts": {
    "setup": "npm install",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "test-only": "cross-env CI=true react-scripts-ts test --env=jsdom",
    "typedoc": "typedoc --ignoreCompilerErrors --out ./docs --mode modules --module commonjs --tsconfig ./tsconfig.json ./src/",
    "posttypedoc": "shx cp .nojekyll docs/.nojekyll",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/node": "^10.11.7",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "cross-env": "^5.2.0",
    "typescript": "^3.1.3"
  }
}
